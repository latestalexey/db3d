%nav.navbar.navbar-static-top.navbar-inverse
  .container-fluid
    - # Brand and toggle get grouped for better mobile display
    .navbar-header
      %button.navbar-toggle.collapsed{type: "button", 'data-toggle': "collapse", 'data-target': "#db3d-navbar-collapse"}
        %span.sr-only Toggle navigation
        - 3.times do
          %span.icon-bar
      %a.navbar-brand{href: "/"} db3d
    .collapse.navbar-collapse#db3d-navbar-collapse
      %ul.nav.navbar-nav.navbar-right
        = render "dev"
        = render me ? 'me' : 'welcome'
